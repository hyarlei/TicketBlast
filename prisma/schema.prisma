generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Ticket {
  id        String   @id @default(uuid())
  orderId   Int
  name      String
  email     String
  type      String
  status    String   @default("PENDING")
  createdAt DateTime @default(now())
}